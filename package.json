{
  "name": "ultimate-saas-ts",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn generate-types --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:ci": "graphql-codegen --config codegen.yml && yarn prisma:generate && yarn prisma:migrate:prod && next build --debug",
    "prisma:migrate:dev": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:prod": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma migrate deploy",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:seed": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma db seed",
    "prisma": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' dotenv -e .env.local -- prisma",
    "stripe:dev": "stripe listen --forward-to  http://localhost:3000/api/stripe/webhook",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "generate-types": "DEBUG=1 graphql-codegen --config codegen.yml"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@hookform/resolvers": "^2.8.2",
    "@next-auth/prisma-adapter": "^0.5.2-next.5",
    "@prisma/client": "^3.2.1",
    "@stripe/stripe-js": "^1.17.1",
    "apollo-server-micro": "^3.3.0",
    "babel-plugin-superjson-next": "^0.4.1",
    "class-validator": "^0.13.1",
    "classnames": "^2.3.1",
    "dotenv-cli": "^4.0.0",
    "firebase": "^9.1.1",
    "firebase-admin": "^9.12.0",
    "graphql": "^15.6.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.11.1",
    "graphql-type-json": "^0.3.2",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "11.1.2",
    "next-connect": "^0.10.2",
    "nodemailer": "^6.6.3",
    "nookies": "^2.5.2",
    "prisma": "^3.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "stripe": "^8.176.0",
    "superjson": "^1.7.5",
    "type-graphql": "^1.1.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript-operations": "^2.1.6",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "@types/micro-cors": "^0.1.2",
    "@types/react": "17.0.21",
    "autoprefixer": "^10.3.4",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "concurrently": "^6.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.15",
    "ts-node": "^10.2.1",
    "typegraphql-prisma": "^0.16.2",
    "typescript": "4.4.3"
  }
}
